<h1>Register</h1>

<%= form_for @user, url: register_path do |f| %>
  First name: <%= f.text_field :first_name, autocomplete: "off" %>
  Last name: <%= f.text_field :last_name, autocomplete: "off" %>
  Username: <%= f.text_field :username, autocomplete: "off" %>
  Password: <%= f.text_field :password, type: "password" %>
  <%= f.submit %>
<% end %>